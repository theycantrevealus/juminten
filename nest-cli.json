{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/gateway_main/src",
  "compilerOptions": {
    "outDir": "dist",
    "webpack": true,
    "webpackConfigPath": "webpack.config.js",
    "tsConfigPath": "apps/gateway_main/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/main",
  "projects": {
    "gateway_main": {
      "type": "application",
      "root": "apps/gateway_main",
      "entryFile": "main",
      "sourceRoot": "apps/gateway_main/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway_main/tsconfig.app.json"
      }
    },
    "consumer_redeem": {
      "type": "application",
      "root": "apps/consumer_redeem",
      "entryFile": "main",
      "sourceRoot": "apps/consumer_redeem/src",
      "compilerOptions": {
        "tsConfigPath": "apps/consumer_redeem/tsconfig.app.json"
      }
    }
  }
}
